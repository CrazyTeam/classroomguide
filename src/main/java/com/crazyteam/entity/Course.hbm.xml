<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!--管理员实体类与数据库的映射-->

<hibernate-mapping>
  <class name="com.crazyteam.entity.Course" table="course"> 
     <id name="id" column="id" type="integer"></id>
    <property name="name" column="name" type="string"></property>
    <property name="teacher"  column="teacher" type="string"></property>
    <property name="startWeek" column="startweek" type="integer"></property>
    <property name="endWeek" column="endweek" type="integer"></property>
    </class>
    
</hibernate-mapping>