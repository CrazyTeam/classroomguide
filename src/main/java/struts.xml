<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>

	<package name="user" extends="struts-default" namespace="/user">
		<action name="login" class="userAction" method="login">
			<result name="success" >/main.html</result>
			<result name="login" >/login.html</result>
			<result name="a" >/adminhome.html</result>
		</action>
	</package>
	<package name="Admin" extends="struts-default" namespace="/">
		<action name="login" class="AdminAction" method="login">
			<result name="success" >/main.html</result>
			<result name="login" >/login.html</result>
			<result name="a" >/adminhome.html</result>
		</action>
		<action name="logout" class="userAction" method="logout">
			<result>/login.html</result>
		</action>
		<action name="register" class="userAction" method="register">
			<result name="success" type="">/login.html</result>
			<result name="error">/error.jsp</result>
		</action>
	</package>
	<package name="admin" extends="struts-default"   namespace="/admin">
		<action name="login" class="adminAction" method="login" >
			<result></result>
		</action>
		<action name="logout" class="adminAction" method="logout">
			<result></result>
		</action>
	</package>


